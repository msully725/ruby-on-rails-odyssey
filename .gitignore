# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
db/*.sqlite3
db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
# Ignore uploaded files in development.
/tmp/storage/*
!/tmp/storage/

# Ignore cache files.
/tmp/cache/*
!/tmp/cache/

# Ignore socket files.
/tmp/sockets/*
!/tmp/sockets/

# Ignore the database schema.
/db/schema.rb
/db/structure.sql

# Ignore all test files.
/test/tmp/*
/test/version_snapshots/*

# Ignore all byebug command history files.
.byebug_history

# Ignore node_modules
/node_modules

# Ignore Yarn files
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore dotenv environment variable files
.env
.env.*

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore coverage reports generated by simplecov.
/coverage/

# Ignore system test files
/system/test/tmp
/system/test/tmp/pids
/system/test/tmp/cache
/system/test/tmp/sockets

# Ignore Spring files.
/spring/*.pid